{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.16.0/.schema/devbox.schema.json",
  "packages": {
    "which": {
      "version": "latest"
    },
    "ncurses": {
      "version": "latest"
    },
    "dotnet-sdk_10": {
      "version": "latest"
    },
    "postgresql": {
      "version": "latest"
    },
    "docker": {
      "version": "latest"
    },
    "bun": {
      "version": "latest"
    },
    "git": {
      "version": "latest"
    },
    "opentofu": {
      "version": "latest"
    },
    "scc": {
      "version": "latest"
    },
    "tokei": {
      "version": "latest"
    },
    "onefetch": {
      "version": "latest"
    },
    "git-sizer": {
      "version": "latest"
    },
    "git-fame": {
      "version": "latest"
    },
    "gitleaks": {
      "version": "latest"
    },
    "trufflehog": {
      "version": "latest"
    },
    "delta": {
      "version": "latest"
    },
    "lazygit": {
      "version": "latest"
    },
    "git-quick-stats": {
      "version": "latest"
    },
    "ruby": {
      "version": "latest"
    },
    "github-linguist": {
      "version": "latest"
    },
    "doppler": {
      "version": "latest"
    }
  },
  "shell": {
    "init_hook": [
      "export ELAVIEW_ENVIRONMENT=${ELAVIEW_ENVIRONMENT:-development}",
      "export ELAVIEW_DEVBOX_ROOT=\"$(git rev-parse --show-toplevel 2>/dev/null || pwd)\"",
      "export ELAVIEW_EV_CMD=\"$ELAVIEW_DEVBOX_ROOT/scripts/cmd\"",
      "export PATH=\"$ELAVIEW_DEVBOX_ROOT/scripts:$PATH\"",
      ". \"$ELAVIEW_DEVBOX_ROOT/scripts/init/env.sh\"",
      ". \"$ELAVIEW_DEVBOX_ROOT/scripts/init/clean.sh\""
    ]
  }
}
