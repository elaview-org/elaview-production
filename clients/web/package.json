{
  "name": "elaview-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "use-bun-instead",
    "yarn": "use-bun-instead",
    "pnpm": "use-bun-instead"
  },
  "scripts": {
    "dev": "next dev",
    "compile": "next build && cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public",
    "start": "bun .next/standalone/server.js ",
    "lint": "eslint --max-warnings 0",
    "typecheck": "next typegen && tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "gql:codegen": "graphql-codegen --config src/gql-codegen.ts",
    "gql:codegen:watch": "graphql-codegen --config src/gql-codegen.ts --watch",
    "test": "bun test .test.ts",
    "test:watch": "bun run test --watch",
    "test:coverage": "bun run test --coverage",
    "test:e2e": "playwright test --config src/playwright.config.ts"
  },
