{
  "name": "elaview-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:prod": "eas build --profile production --platform all",
    "codegen": "graphql-codegen --config src/utils/codegen.ts",
    "codegen:watch": "graphql-codegen --config src/utils/codegen.ts --watch"
  },
  "dependencies": {
    "@apollo/client": "^4.1.2",
    "@expo/vector-icons": "^15.0.3",
    "date-fns": "^4.1.0",
    "expo": "~54.0.31",
    "expo-auth-session": "~7.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "^6.0.20",
    "expo-image-picker": "^17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "^19.0.8",
    "expo-router": "~6.0.22",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "^1.26.20",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-react-apollo": "^4.4.0",
    "@parcel/watcher": "^2.5.4",
    "@types/react": "~19.1.17",
    "babel-preset-expo": "^54.0.10",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3"
  },
  "private": true
}
